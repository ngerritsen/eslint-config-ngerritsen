{
  "rules": {
    "init-declarations": 0,
    "no-catch-shadow": 2,
    "no-label-var": 2,
    "no-restricted-globals": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-undef-init": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false,
        "classes": true
      }
    ]
  }
}
